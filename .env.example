# LLM Keys
OPENROUTER_API_KEY=...
HF_TOKEN=...
GEMINI_API_KEY=...

# Tool Keys
SERPER_API_KEY=...
TAVILY_API_KEY=...
SERPAPI_API_KEY=...

# MLflow Tracking
ENABLE_TRACING=true

# Database (MongoDB Atlas)
# Format: mongodb+srv://{MONGO_USERNAME}:{MONGO_PASSWORD}@{MONGO_CLUSTER}/?retryWrites=true&w=majority&appName={MONGO_APP_NAME}
# Note: If your password contains special characters, need to change
# eg. @, change to %40
MONGO_USERNAME=<your_mongodb_username>
MONGO_PASSWORD=<your_mongodb_password>
MONGO_CLUSTER=<your_cluster_name.mongodb.net>
MONGO_APP_NAME=<your_app_name>
DB_NAME=<your_database_name>

# Lightweight LLM for rolling summary of chat history
# Default: gemini/gemini-flash-latest (requires GEMINI_API_KEY)
SUMMARY_MODEL=gemini/gemini-flash-latest

# FastAPI API URL
API_URL=http://localhost:8000
